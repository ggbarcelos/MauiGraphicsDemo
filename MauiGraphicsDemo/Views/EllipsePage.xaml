<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:drawables="clr-namespace:MauiGraphicsDemo.Drawables"
             x:Class="MauiGraphicsDemo.Views.EllipsePage"
             Title="Elipse">
    <ContentPage.Resources>
        <drawables:EllipseDrawable x:Key="ellipseDrawable"/>
    </ContentPage.Resources>
    <GraphicsView Drawable="{StaticResource ellipseDrawable}"
                  x:Name="EllipseGraphicsView">
    </GraphicsView>
</ContentPage>
