<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:drawables="clr-namespace:MauiGraphicsDemo.Drawables"
             x:Class="MauiGraphicsDemo.Views.PathPage"
             Title="Path">
    <ContentPage.Resources>
        <drawables:PathDrawable x:Key="pathDrawable"/>
    </ContentPage.Resources>
    <GraphicsView Drawable="{StaticResource pathDrawable}"
                  x:Name="PathGraphicsView">
    </GraphicsView>
</ContentPage>
